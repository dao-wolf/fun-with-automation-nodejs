{
  "name": "test-automation-javascript",
  "version": "1.0.0",
  "description": "test automation with javascript",
  "scripts": {
    "lint": "eslint ui-tests/** --ext=js",
    "selenium:setup": "selenium-standalone install && selenium-standalone start",
    "selenium:start": "node exec-selenium.js",
    "selenium:stop": "kill $(ps -e | egrep '[s]elenium' | awk '{print $1}')",
    "test": "mocha --opts ./mocha.opts 'ui-tests/**/*.spec.js'  --require ./setup.js"
  },
  "main": "index.js",
  "repository": "git@github.com:opantsjoha/test-automation-javascript.git",
  "author": "opantsjoha <opantsjoha@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-register": "^6.26.0",
    "selenium-standalone": "^6.16.0",
    "selenium-webdriver": "^4.0.0-alpha.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha": "^6.1.4"
  }
}
